print("=== Shellcode Tester ===")

# 常見 shellcode patterns
execve_syscall = 59
sh_string_addr = 140737349541256

print("Testing shellcode components...")

# 測試 1: 系統調用號
print("Test 1: Syscall Numbers")
print(execve_syscall)
print(hex(execve_syscall))

# 測試 2: 字符串地址
print("Test 2: String Addresses")
print(hex(sh_string_addr))

# 測試 3: 打包地址
packed = p64(sh_string_addr)
print("Test 3: Pack/Unpack")
print(hex(unpack64(packed)))

# 測試 4: 模擬 shellcode 構建
print("Test 4: Building shellcode stub")
shellcode = []
shellcode = append(shellcode, 72)
shellcode = append(shellcode, 184)
shellcode = append(shellcode, 59)
shellcode = append(shellcode, 0)
shellcode = append(shellcode, 0)
shellcode = append(shellcode, 0)

print("Shellcode length:")
print(len(shellcode))

# 顯示 shellcode
print("Shellcode bytes:")
i = 0
while i < len(shellcode):
    byte_val = shellcode[i]
    print(hex(byte_val))
    i = i + 1

print("[SUCCESS] Shellcode test completed!")
